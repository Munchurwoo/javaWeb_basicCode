package github_testCode;

public class testCode {
	public static void main(String[] args) {
		System.out.printf("%.0f \n", 0.123);
		System.out.printf("%1.1f \n", 123.123);
		System.out.printf("%2.2f \n", 132.123);
		int x = 10; 
		System.out.println(x%=9);
		
		int i = 10; int z = 20;
		int result = i>z ? 100:300;
		
		System.out.println(i>z ? 100:200);
		System.out.println(i<z ? 100:200);
		System.out.println(result);
	}
}
